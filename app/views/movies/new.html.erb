<h1>New List</h1>

<%= form_with(model: @list, local: true) do |form| %>
  <%= form.label :name %>
  <%= form.text_field :name, class: 'form-control' %>

  <%= form.submit 'Create List', class: 'btn btn-primary' %>
<% end %>

<%= link_to 'Back to Lists', lists_new_path, class: 'btn btn-secondary' %>
